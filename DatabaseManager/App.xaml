<Application x:Class="DatabaseManager.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             Startup="AppStartup">
  <Application.Resources>
    <ResourceDictionary>
      <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary x:Name="VertexType" Source="Assets/VertexType.xaml"/>
        <ResourceDictionary x:Name="edgeType" Source="Assets/EdgeType.xaml"/>
        <ResourceDictionary x:Name="vertex" Source="Assets/Vertex.xaml"/>
      </ResourceDictionary.MergedDictionaries>
    </ResourceDictionary>
  </Application.Resources>
</Application>
